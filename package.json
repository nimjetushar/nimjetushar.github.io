{
  "name": "nimjetushar.github.io",
  "version": "1.0.0",
  "private": true,
  "author": "Tushar Nimje",
  "license": "apache-2.0",
  "dependencies": {
    "font-awesome": "4.7.0",
    "nuka-carousel": "4.4.1",
    "react": "16.5.2",
    "react-dom": "16.4.2",
    "react-router-dom": "4.3.1"
  },
  "devDependencies": {
    "del": "3.0.0",
    "gulp": "3.9.1",
    "node-sass-chokidar": "1.3.3",
    "npm-run-all": "4.1.3",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js && npm run move",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "move": "gulp"
  },
  "description": "Github profile (https://nimjetushar.github.io/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nimjetushar/nimjetushar.github.io.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/nimjetushar/nimjetushar.github.io/issues"
  },
  "homepage": "https://nimjetushar.github.io/"
}
