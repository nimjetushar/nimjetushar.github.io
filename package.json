{
  "name": "nimjetushar.github.io",
  "version": "1.0.0",
  "author": "Tushar Nimje",
  "license": "apache-2.0",
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "@fortawesome/fontawesome-free": "5.9.0",
    "nuka-carousel": "4.5.8",
    "react": "16.12.0",
    "react-dom": "16.8.6",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.4.5",
    "@babel/core": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "2.0.1",
    "css-loader": "3.0.0",
    "eslint": "6.5.1",
    "eslint-plugin-react": "7.14.2",
    "file-loader": "4.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.13.1",
    "mini-css-extract-plugin": "0.7.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "sass-loader": "7.3.1",
    "style-loader": "1.0.0",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack-cli": "3.3.5",
    "webpack": "4.35.2",
    "webpack-dev-server": "3.7.2",
    "webpack-merge": "4.2.2",
    "workbox-webpack-plugin": "4.3.1"
  },
  "scripts": {
    "start": "webpack-dev-server --env.development --hot",
    "build": "webpack --env.production",
    "lint": "eslint --ext .js,.jsx src/**"
  },
  "description": "Github profile (https://nimjetushar.github.io/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nimjetushar/nimjetushar.github.io.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/nimjetushar/nimjetushar.github.io/issues"
  },
  "homepage": "https://nimjetushar.github.io/",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "buildconfig": {
    "port": 3000
  }
}
